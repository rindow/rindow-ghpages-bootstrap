---
layout: null
---
{% if site.url %}<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<url>
  <loc>{{ site.url }}/</loc>
</url>
{% for p in site.pages %}{% capture ext %}{{ p.url | split:"." | last }}{% endcapture %}{% if ext == "css" or ext == "xml" or p.url == "/robots.txt" or p.url == "/404.html" %}{% else %}<url>
  <loc>{{ site.url }}{{ p.url }}</loc> 
</url>
{% endif %}{% endfor %}{% for collection in site.collections %}{% if collection[1].output %}{% assign name = collection[0] %}{% for doc in site[name] %}<url>
  <loc>{{ site.url }}{{ doc.url }}</loc>
</url>
{% endfor %}{% endif %}{% endfor %}</urlset>{% endif %}